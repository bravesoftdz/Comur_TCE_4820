  object sdsValores: TZReadOnlyQuery
    Connection = dmDatabase_EBS.ZConnection1
    SQL.Strings = (
      
        '  SELECT pro.tipo, fun.cd_funcionario COD_FUNCIONARIO, fun.nome ' +
        'NOME, '#10'  ffun.nr_agencia_deposito AGENCIA, '
      
        'ffun.cd_banco_deposito,'#10'concat(cast(ffun.nr_conta_deposito  as c' +
        'har(9)),ffun.dv_conta_deposito) as CONTA_CORRENTE,'#10'sum(pro.valor' +
        ') valor , '
      'eve.tp_evento'#10'  '
      'FROM procevento pro '#10'  '
      
        'INNER JOIN funcionario fun ON fun.cd_funcionario = pro.cd_funcio' +
        'nario'#10'  and  fun.cd_empresa = 1'#10'  '
      
        'INNER JOIN eventog eve ON eve.cd_evento = pro.cd_evento '#10'  LEFT ' +
        'JOIN funfuncional ffun ON ffun.cd_funcionario = fun.cd_funcionar' +
        'io '#10'and ffun.cd_empresa = 1'#10'  '
      'WHERE pro.mes = 01 AND pro.ano = 2020'#10'   '
      'AND ffun.cd_banco_deposito = 1 AND pro.cd_empresa = 1'#10'   '
      
        'AND pro.tipo = '#39'2'#39#10'   AND (eve.tp_evento = '#39'V'#39' or eve.tp_evento ' +
        '= '#39'D'#39' )'#13#10#10
      
        'group by pro.tipo, fun.cd_funcionario, fun.nome,  ffun.nr_agenci' +
        'a_deposito, '#10'ffun.nr_conta_deposito, ffun.dv_conta_deposito, eve' +
        '.tp_evento   , ffun.cd_banco_deposito'#10'   '#10)
    Params = <>
    Left = 96
    Top = 152
  end
